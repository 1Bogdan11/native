      li.sale-points__item 
        +point-info({
            title: "Магазин на Большой Никитской",
            type: "contacts",
            id: "2"
        })
      li.sale-points__item 
        +point-info({
            title: "Шоурум на Авангардной",
            type: "contacts",
            id: "3"
        })
      li.sale-points__item 
