        +point-info({
            title: "Шоурум на Лиговском",
            type: "contacts",
            id: "5"
        })
      li.sale-points__item 
        +point-info({
            title: "Шоурум на Беговой",
            type: "contacts",
            id: "6"
        })
    ul.sale-points__list.js-sale-point(data-region-id="3")
